{
    "name": "mostlyserious/ms-craft",
    "description": "Craft CMS",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:mostlyserious/craft-block-audit.git"
        }
    ],
    "require": {
        "craftcms/aws-s3": "2.0.1",
        "craftcms/cms": "4.2.4",
        "craftcms/redactor": "3.0.2",
        "fortrabbit/craft-copy": "2.1.1",
        "hillholliday/craft-user-manual": "4.0.0",
        "nystudio107/craft-seomatic": "4.0.7",
        "percipioglobal/craft-colour-swatches": "4.2.0.1",
        "putyourlightson/craft-sendgrid": "2.0.1",
        "sebastianlenz/linkfield": "2.1.4",
        "spicyweb/craft-embedded-assets": "3.0.5",
        "spicyweb/craft-neo": "3.3.8",
        "verbb/icon-picker": "2.0.0-beta.5",
        "verbb/smith": "2.0.0",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true,
            "fortrabbit/craft-auto-migrate": true,
            "treeware/plant": true
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"copy('.env.example', '.env');\"",
            "@php -r \"unlink('composer.json');\"",
            "@php -r \"rename('composer.json.default', 'composer.json');\"",
            "@composer dump-autoload -o",
            "@php craft setup/security-key",
            "@php craft setup/app-id"
        ]
    }
}
