{
    "name": "mostlyserious/ms-craft",
    "description": "Craft CMS",
    "type": "project",
    "require": {
        "craftcms/aws-s3": "1.3.0",
        "craftcms/cms": "3.7.20",
        "craftcms/redactor": "2.8.8",
        "fortrabbit/craft-copy": "1.0.7",
        "hillholliday/craft-user-manual": "2.1.0",
        "nystudio107/craft-seomatic": "3.4.17",
        "percipioglobal/craft-colour-swatches": "1.4.2",
        "putyourlightson/craft-sendgrid": "1.2.3",
        "sebastianlenz/linkfield": "1.0.25",
        "spicyweb/craft-neo": "2.11.17",
        "superbig/craft3-imgix": "2.1.0",
        "verbb/icon-picker": "1.1.12",
        "verbb/smith": "1.1.14",
        "verbb/super-table": "2.6.8",
        "vlucas/phpdotenv": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"copy('.env.example', '.env');\"",
            "@php -r \"unlink('composer.json');\"",
            "@php -r \"rename('composer.json.default', 'composer.json');\"",
            "@composer dump-autoload -o",
            "@php craft setup/welcome"
        ]
    }
}
